{
  "manifest_version": 3,
  "name": "Slash Focus",
  "version": "0.1.0",
  "description": "Press the slash key to focus on a specific text box.",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "author": "koyashiro",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-script.js"]
    }
  ],
  "permissions": ["activeTab"]
}
